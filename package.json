    "@mui/material": "^7.3.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^7.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-i18next": "^14.1.2",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "recharts": "^3.4.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.4",
    "@lhci/cli": "^0.12.0",
    "@playwright/test": "^1.40.0",
    "jest-axe": "^8.0.0",
    "msw": "^2.0.0",
    "web-streams-polyfill": "^4.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:unit": "react-scripts test --testPathIgnorePatterns=integration --coverage --watchAll=false",
    "test:integration": "react-scripts test --testPathPattern=integration --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:accessibility": "react-scripts test --testPathPattern=accessibility --watchAll=false",
    "test:security": "react-scripts test --testPathPattern=security --watchAll=false",
    "test:i18n": "react-scripts test --testPathPattern=i18n --watchAll=false",
    "test:all": "npm run test:coverage && npm run test:integration && npm run test:e2e",
    "lighthouse": "lhci autorun",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-router|react-router-dom|@remix-run|axios|msw|@mswjs|until-async|web-streams-polyfill|@mswjs/interceptors)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.{ts,tsx}",
      "!src/index.tsx",
      "!src/reportWebVitals.ts"
    ]
  }
}
